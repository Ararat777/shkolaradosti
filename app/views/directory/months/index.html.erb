<h4 class="text-center">Месяца</h4>
  <table class="table">
    <thead>
      <tr>
        <th>Месяц</th>
        <th>Год</th>
        <th>Количество рабочих дней</th>
        <th>Действия</th>
      </tr>
    </thead>
    <tbody>
      <% @months.each do |item| %>
      <tr>
        <td><%= item.title %></td>
        <td><%= item.year %></td>
        <td><%= item.work_days_size %></td>
        <td><%= link_to "Просмотреть",directory_month_path(item.id) %></td>
      </tr>
      <% end %>
    </tbody>
</table>