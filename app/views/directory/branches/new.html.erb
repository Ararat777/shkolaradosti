<div class="row">
  <div class="col-md-6 offset-md-2">
    <%= form_for @branch,url: directory_branches_path do |f| %>
    <div class="form-group">
      <%= f.label "Название" %>
      <%= f.text_field :title, class: "form-control" %>
    </div>
    <div class="form-group">
      <%= f.label "Адресс" %>
      <%= f.text_field :adress,class: "form-control" %>
    </div>
    <%= f.submit "Добавить", class: "btn btn-success form-control" %>
    <% end %>
  </div>
</div>
