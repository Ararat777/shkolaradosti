<table class="table">
      <thead>
        <tr>
          <th>Сумма</th>
          <th>Комментарий</th>
          <th>Время</th>
          <th>Действия</th>
        </tr>
      </thead>
      <tbody>
        <% @encashments.each do |item| %>
        <tr>
          <td><%= item.amount %>грн</td>
          <td><%= item.comment %></td>
          <td><%= item.created_at.strftime("%F %T") %></td>
          <td><%= link_to "Просмотреть", encashment_path(item.id) %></td>
        </tr>
        <% end %>
      </tbody>
    </table>